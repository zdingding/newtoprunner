<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="@color/setting_logout_pressed" android:endColor="@color/setting_logout_pressed" android:angle="0.0" android:centerColor="@color/setting_logout_pressed" />
            <stroke android:width="@dimen/public_divide_line" android:color="@color/public_divide_heavy_line" />
            <solid android:color="@color/setting_logout_pressed" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="@dimen/public_divide_line" android:color="@color/public_divide_heavy_line" />
            <solid android:color="@color/public_app_bg" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
</selector>